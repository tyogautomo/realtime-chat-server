{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": [
            "warn",
            "always"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "camelcase": "error",
        "max-len": [
            "error",
            {
                "code": 200
            }
        ]
    }
}